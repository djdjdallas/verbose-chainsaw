# Mobile App Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Supabase
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Backend API
EXPO_PUBLIC_API_URL=https://your-backend-domain.com

# RevenueCat
EXPO_PUBLIC_REVENUECAT_IOS_KEY=your_ios_key_here
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your_android_key_here

# Mixpanel (Optional)
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token_here

# Environment
NODE_ENV=development
